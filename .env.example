# IBKR 配置
IBKR_HOST=host.docker.internal
IBKR_PORT=7497
IBKR_CLIENT_ID=999

# 交易配置
SYMBOL=MNQ
EXCHANGE=CME  
CURRENCY=USD
CONTRACT_MONTH=202603  # 合约到期月，空字符串表示当前活跃合约

# 风险管理
RISK_PERCENTAGE=1.0      # 单笔风险百分比 (1.0 = 1%)
MAX_POSITION_SIZE=10      # 最大持仓手数
DAILY_LOSS_LIMIT=3.0     # 日最大亏损百分比 (3.0 = 3%)

# 策略参数
TIMEFRAME_15M=900        # 15分钟时间框架 (秒)
TIMEFRAME_1H=3600        # 1小时时间框架 (秒)  
TIMEFRAME_4H=14400       # 4小时时间框架 (秒)
FVG_SENSITIVITY=0.8      # FVG 敏感度 (0.0-1.0, 越高越敏感)
LIQUIDITY_THRESHOLD=2.0  # 流动性阈值 (倍数)

# 日志配置
LOG_LEVEL=INFO

# 回测配置
BACKTEST_START_DATE=2025-01-01
BACKTEST_END_DATE=2026-01-01